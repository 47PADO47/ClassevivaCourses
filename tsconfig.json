{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src",
    "downlevelIteration": true,
    "importHelpers": false,
    "module": "es2020",
    "outDir": "./dist/",
    "sourceMap": false,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "target": "es6",
    "lib": ["es2017", "dom"],
    "strict": true,
    "allowJs": true,
  }
}
